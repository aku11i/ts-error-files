{
  "name": "ts-error-files",
  "version": "0.0.10",
  "description": "",
  "type": "module",
  "bin": {
    "ts-error-files": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node --experimental-import-meta-resolve dist/cli.js",
    "build": "shx rm -rf dist && tsc",
    "dev": "node --experimental-import-meta-resolve --loader=ts-node/esm/transpile-only src/cli.ts",
    "test": "npm run test:e2e",
    "test:unit": "node --test --experimental-import-meta-resolve --loader=ts-node/esm/transpile-only src/find-ts-lib-folder.test.ts",
    "test:e2e": "node --test --experimental-import-meta-resolve --loader=ts-node/esm/transpile-only e2e.test.ts",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [],
  "author": "aku11i",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.2.5",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "typescript": "*"
  },
  "dependencies": {
    "ts-morph": "^18.0.0"
  }
}
